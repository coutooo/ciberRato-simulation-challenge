mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: Viewer.pro  /usr/lib/qt3/mkspecs/default/qmake.conf /usr/lib/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile Viewer.pro
qmake: 
	@$(QMAKE) -o Makefile Viewer.pro

dist: 
	@mkdir -p .tmp/Viewer && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/Viewer/ && ( cd `dirname .tmp/Viewer` && $(TAR) Viewer.tar Viewer && $(GZIP) Viewer.tar ) && $(MOVE) `dirname .tmp/Viewer`/Viewer.tar.gz . && $(DEL_FILE) -r .tmp/Viewer

